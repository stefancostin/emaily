  "bugs": {
    "url": "https://github.com/stefancostin/emaily-api/issues"
  },
  "homepage": "https://github.com/stefancostin/emaily-api#readme",
  "dependencies": {
    "concurrently": "^5.2.0",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.25",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  }
