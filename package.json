{
  "name": "emaily-api",
  "version": "1.0.0",
  "description": "Node.js API for an email campaign application",
  "main": "index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
